{
  "name": "rossina",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 7777",
    "build": "next build",
    "start": "next start -p 7777",
    "apollo:generate": "apollo codegen:generate --target typescript --excludes=node_modules/* --includes=**/*.tsx --endpoint http://localhost:3000/api/graphql --tagName=gql --outputFlat types/generated",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "3.3.15",
    "@apollo/link-error": "2.0.0-beta.3",
    "@apollo/react-ssr": "4.0.0",
    "@brainhubeu/react-carousel": "^2.0.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "apollo": "^2.32.11",
    "apollo-upload-client": "14.1.3",
    "babel-core": "7.0.0-bridge.0",
    "date-fns": "2.21.1",
    "dotenv": "8.2.0",
    "downshift": "6.1.3",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "graphql-upload": "11.0.0",
    "lodash.debounce": "4.0.8",
    "next": "10.1.3",
    "next-with-apollo": "5.1.1",
    "nprogress": "0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-loader-spinner": "^4.0.0",
    "react-transition-group": "4.4.1",
    "styled-components": "5.2.3",
    "waait": "1.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.5",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-react-apollo": "^2.2.4",
    "@types/brainhubeu__react-carousel": "^2.0.2",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.4",
    "babel-plugin-styled-components": "^1.12.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
